{
  "name": "deepseek-mcp-server",
  "version": "0.3.0",
  "description": "Model Context Protocol server supporting multiple AI providers including DeepSeek, OpenAI, Claude, Mistral, Codestral, and Gemini.",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "deepseek-mcp-server": "./build/index.js"
  },
  "files": [
    "build/**/*",
    "assets/favicon.ico",
    "assets/favicon-32x32.png",
    "assets/apple-touch-icon.png"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "prepublishOnly": "npm run build",
    "inspect": "npx @modelcontextprotocol/inspector build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.2.2"
  },
  "author": "DMontgomery40",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DMontgomery40/deepseek-mcp-server"
  },
  "keywords": [
    "mcp",
    "deepseek",
    "openai",
    "claude",
    "mistral",
    "codestral",
    "gemini",
    "ai",
    "llm",
    "multi-provider"
  ]
}
